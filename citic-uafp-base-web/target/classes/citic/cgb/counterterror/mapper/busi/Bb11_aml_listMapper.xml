<?xml version="1.0" encoding="UTF-8"?> 
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd"> 
 
<mapper namespace="citic.cgb.counterterror.mapper.busi.Bb11_aml_listMapper"> 
 
 <sql id="base_columns"> 
    md_code,source_id,pname,idtype,idvalue,san_name_type,black_class_cd,black_action,data_date,act_status,remark,send_time,send_flag
 </sql> 
 
  <select id="selectBb11_aml_listByVo" parameterType="citic.cgb.counterterror.domain.Bb11_aml_list" resultType="citic.cgb.counterterror.domain.Bb11_aml_list"> 
   select 
   <include refid="base_columns" /> 
   from bb11_aml_list 
    <where> 
      <if test="md_code != null  and md_code != '' "> 
         md_code = #{md_code} 
      </if> 
    </where> 
  </select> 
 
  <insert id="insertBb11_aml_list" parameterType="citic.cgb.counterterror.domain.Bb11_aml_list"> 
    insert into bb11_aml_list(
    <include refid="base_columns" /> 
    ) values(
    #{md_code},#{source_id},#{pname},#{idtype},#{idvalue},#{san_name_type},#{black_class_cd},#{black_action},#{data_date},#{act_status},#{remark},#{send_time},#{send_flag}
    ) 
    </insert> 
 
  <update id="updateBb11_aml_list" parameterType="citic.cgb.counterterror.domain.Bb11_aml_list"> 
  update bb11_aml_list 
  <set> 
  <if test="md_code != null">
md_code=#{md_code},
</if> 
<if test="source_id != null">
source_id=#{source_id},
</if> 
<if test="pname != null">
pname=#{pname},
</if> 
<if test="idtype != null">
idtype=#{idtype},
</if> 
<if test="idvalue != null">
idvalue=#{idvalue},
</if> 
<if test="san_name_type != null">
san_name_type=#{san_name_type},
</if> 
<if test="black_class_cd != null">
black_class_cd=#{black_class_cd},
</if> 
<if test="black_action != null">
black_action=#{black_action},
</if> 
<if test="data_date != null">
data_date=#{data_date},
</if> 
<if test="act_status != null">
act_status=#{act_status},
</if> 
<if test="remark != null">
remark=#{remark},
</if> 
<if test="send_time != null">
send_time=#{send_time},
</if> 
<if test="send_flag != null">
send_flag=#{send_flag}
</if> 

  </set> 
  where md_code=#{md_code} 
  </update> 
 
  <delete id="deleteBb11_aml_listByID" parameterType="String">  
  <![CDATA[ delete from bb11_aml_list where md_code=#{md_code} ]]>
   </delete> 
 
  <select id="selectBb11_aml_listByID" parameterType="String" resultType="citic.cgb.counterterror.domain.Bb11_aml_list"> 
  select 
  <include refid="base_columns" /> 
  from bb11_aml_list 
  where md_code=#{md_code} 
  </select> 
 
</mapper> 
